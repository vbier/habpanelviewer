    <string name="intro_welcome">Welcome</string>
    <string name="intro_welcome_text">Welcome to HABPanelViewer, a kiosk browser with some additional openHAB related features.</string>
    <string name="intro_browser">Browser</string>
    <string name="intro_browser_text">HPV can show your HABPanel in full screen mode. Open the context menu with long-press to toggle kiosk-mode.</string>
    <string name="intro_reporting">Reporting</string>
    <string name="intro_reporting_text">HPV can send device data, like sensor values or detected motion to openHAB.</string>
    <string name="intro_commanding">Commanding</string>
    <string name="intro_commanding_text">HPV can control your device on commands sent from openHAB. Turn the screen on or off, toggle bluetooth and more…</string>
    <string name="intro_serverConfigured">Your openHAB server URL is already configured to %s.</string>
    <string name="intro_configuration">Configuration</string>
    <string name="intro_configuration_text">You can configure HPV by opening the menu and selecting Preferences. To open the menu, swipe with your finger from the right side of the screen to the middle.</string>
    <string name="intro_credentials">Credentials Storage</string>
    <string name="intro_credentials_text">HPV can store your basic authentication (BA) credentials so you only need to log in once to websites using BA.</string>
    <string name="intro_credentials_text2">Credentials are stored unencrypted in a database in a private part of the file system that can only be read by root when you device is rooted.</string>
    <string name="intro_help">Help</string>
    <string name="intro_help_text">Open help from the menu to access the manual or visit the wiki or support forum.</string>
    <string name="intro_ready">Ready</string>
    <string name="intro_ready_text">You are ready to go.</string>
    <string name="intro_pleaseComplete">The server is not yet configured. Please complete the initial configuration.</string>
    <string name="intro_discoveryNotAvailable">Discovery is not available on your device. Please enter the URL in the preferences.</string>

